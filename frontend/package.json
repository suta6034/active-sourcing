{
  "name": "frontend",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest run --config ./tests/drivers/vitest/vitest.config.ts",
    "test:unit:watch": "vitest --config ./tests/drivers/vitest/vitest.config.ts",
    "test:unit:coverage": "vitest run --coverage --config ./tests/drivers/vitest/vitest.config.ts",
    "build-only": "vite build",
    "build:sdev": "vite build --mode sdev",
    "build:staging": "vite build --mode staging",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint:scripts": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:dependencies": "depcruise --config dependency-cruiser.config.js ./src",
    "lint": "run-p lint:scripts lint:dependencies",
    "visualize:dependencies": "depcruise --config dependency-cruiser.config.js -x '(__test__|\\.eslintrc.cjs|\\.spec\\.ts)' --include-only '^src' --output-type dot src | dot -T svg > dependencygraph.svg",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook"
  },
  "dependencies": {
    "@storybook/addon-a11y": "^6.5.13",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "^6.5.13",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@vue/test-utils": "^2.3.0",
    "@vuelidate/core": "^2.0.0-alpha.43",
    "@vuelidate/validators": "^2.0.0-alpha.31",
    "@vueuse/core": "^9.2.0",
    "cypress-downloadfile": "^1.2.3",
    "msw": "^0.43.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.6",
    "vue-slider-component": "^4.1.0-beta.7"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@karriereat/eslint-config": "git+ssh://git@gitlab/js/eslint-config.git#v2.1.0",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-vite": "^0.1.37",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.13",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@testing-library/user-event": "^14.2.1",
    "@testing-library/vue": "^6.6.1",
    "@types/node": "^16.11.43",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "c8": "^7.11.3",
    "cross-fetch": "^3.1.5",
    "cypress": "^12.4.1",
    "cypress-file-upload": "^5.0.8",
    "dependency-cruiser": "^11.11.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.18.0",
    "eslint-import-resolver-typescript": "^3.2.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-storybook": "^0.6.7",
    "eslint-plugin-tailwindcss": "^3.7.1",
    "eslint-plugin-vue": "^9.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^20.0.0",
    "library-util": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.1",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.38.1"
  },
  "engines": {
    "npm": ">=8.0.0 <8.19.0",
    "node": ">=18.0.0 <18.9.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "karriere.at": {
    "managePackage": true,
    "installPackage": false,
    "updateChromeDriver": false
  }
}
